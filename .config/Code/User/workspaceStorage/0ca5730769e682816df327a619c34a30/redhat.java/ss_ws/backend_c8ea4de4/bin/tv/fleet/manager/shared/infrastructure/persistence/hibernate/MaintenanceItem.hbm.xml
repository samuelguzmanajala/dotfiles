<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="tv.fleet.manager.shared.domain.MaintenanceItem" table="maintenance_item">

        <id name="itemId" column="item_id" type="string" length="36" access="field"/>
        <property name="quantity" column="quantity" type="integer" access="field"/>
        <property name="maintenanceId" column="maintenance_id" type="string" length="36" access="field"/>
    </class>
</hibernate-mapping>